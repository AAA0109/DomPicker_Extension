{
  "name": "DOM Picker",
  "version": "2.0",
  "manifest_version": 3,
  "description": "Chrome Extension for Ollacart",
  "permissions": ["storage", "tabs", "activeTab"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["inject.js"],
    "all_frames": true
  }],
  "background": {
    "service_worker": "background.js"
  },
  "action": {  },
  "icons": {
    "32": "icon_32.png",
    "128": "icon_128.png"
  }
}
